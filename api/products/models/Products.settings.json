{
  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "products",
    "description": "Les produits vendus"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "name": {
      "default": "",
      "unique": true,
      "minLength": 3,
      "type": "string",
      "required": true
    },
    "price": {
      "default": 0,
      "max": 100,
      "type": "decimal"
    },
    "description": {
      "default": "",
      "type": "text"
    },
    "type": {
      "default": "",
      "type": "enumeration",
      "required": true,
      "enum": [
        "starter",
        "main",
        "dessert",
        "drink"
      ]
    }
  }
}